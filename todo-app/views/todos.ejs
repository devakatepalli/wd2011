<table border="1">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Due Date</th>
        <th>Status</th>
    </tr>
    <% todos.forEach(todo => { %>
    <tr>
        <td><%= todo.id %></td>
        <td><%= todo.title %></td>
        <td><%= todo.dueDate %></td>
        <td><%= todo.completed ? "✅ Completed" : "❌ Pending" %></td>
    </tr>
    <% }); %>
</table>
